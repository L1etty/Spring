<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.kyung2am.spring.ex.jsp.repository.UserRepository">
 	<insert id="insertUser" parameterType="map">
 		INSERT INTO
 			`new_user`
 			(
 				`name`
 				, `yyyymmdd`
 				, `email`
 				, `introduce`
 			)
 			VALUE
 			(
 				#{name}
 				, #{birthday}
 				, #{email}
 				, #{introduce}
 			)
 			
 		
 	</insert>
 </mapper>